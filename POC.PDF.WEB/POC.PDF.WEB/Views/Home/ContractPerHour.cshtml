
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<meta name="viewport" content="width=device-width" />*@
    <title>Contrato por hora</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="~/Content/bootstrap.css" rel="stylesheet">

    <style type="text/css">
        body {
            padding-top: 60px;
            padding-bottom: 40px;
        }


        .bgcolor {
            background-color: none;
        }

        .lightblue {
            background-color: none;
        }

        .form-horizontal .control-label {
            width: 70px;
        }

        .form-horizontal .controls {
            margin-left: 90px;
        }
    </style>
    <link href="~/Content/bootstrap-responsive.css" rel="stylesheet">

    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="../assets/ico/favicon.ico" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../assets/ico/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../assets/ico/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../assets/ico/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="../assets/ico/apple-touch-icon-57-precomposed.png" />

</head>
<body>
    <div>
        <img src="~/Content/LogoContrato.PNG" alt="Logo do contrato" />
    </div>
    <br />
    <br />
    <div class="container-fluid">
        <form class="form-horizontal">
            <legend><strong>CONTRATANTE</strong></legend>
            <div class="row-fluid">
                <div class="span8">
                    <div class="row-fluid">
                        <div class="span12 bgcolor">
                            <div class="alert alert-error">
                                <a href="#" class="close" data-dismiss="alert">×</a>
                                Error Messages.
                            </div>
                        </div>
                    </div>
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="firstName">EMPRESA</label>
                                <div class="controls">
                                    <input type="text" id="firstName" placeholder="Chee Kin"  class="input-xxlarge">
                                    <span class="help-block">This is inline help</span>
                                </div>
                            </div>
                        </div><!--/span-->
                    </div><!--/row-->
                    <div class="row-fluid">
                        <div class="span12 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">RAZÃO SOCIAL</label>
                                <div class="controls">
                                    <input type="text" id="lastName" class="input-xxlarge" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                    </div>	<!--/row-->					
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">ATIVIDADE</label>
                                <div class="controls">
                                    <input type="text" id="lastName" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                        <div class="span6 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">CNPJ / CPF</label>
                                <div class="controls">
                                    <input type="text" id="lastName" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                    </div><!--/row-->
                    <div class="row-fluid">
                        <div class="span12 bgcolor">
                            <div class="control-group" >
                                <label class="control-label" for="postCode">REPRESENTANTE LEGAL</label>
                                <div class="controls">
                                    <input type="text" class="input-xxlarge" id="reprentante">
                                </div>
                            </div>
                        </div><!--/span-->
                    </div><!--/row-->
                    <div class="row-fluid">
                        <div class="span12 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="street">E-MAIL</label>
                                <div class="controls">
                                    <input type="text" class="input-xxlarge" id="street">
                                </div>
                            </div>
                        </div><!--/span-->
                    </div>
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">CPF</label>
                                <div class="controls">
                                    <input type="text" id="lastName" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                        <div class="span6 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">TELEFONE</label>
                                <div class="controls">
                                    <input type="text" id="lastName" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                    </div><!--/row-->
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group error">
                                <label class="control-label" for="lastName">Cel.</label>
                                <div class="controls">
                                    <input type="text" id="lastName" placeholder="Lim">
                                    <span class="help-block">This field has error.</span>
                                </div>
                            </div>
                        </div><!--/span-->
                    </div>	<!--/row-->

                    <fieldset>
                        <legend><strong>ENDEREÇO</strong></legend>
                        <div class="row-fluid">
                            <div class="span12 bgcolor">
                                <div class="control-group">
                                    <label class="control-label" for="street">Endereço Completo</label>
                                    <div class="controls">
                                        <input type="text" class="input-xxlarge" id="street">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6 bgcolor">
                                <div class="control-group">
                                    <label class="control-label" for="city">Cidade</label>
                                    <div class="controls">
                                        <input type="text" id="city">
                                    </div>
                                </div>
                            </div><!--/span-->
                            <div class="span6 bgcolor">
                                <div class="control-group">
                                    <label class="control-label" for="state">UF</label>
                                    <div class="controls">
                                        <input type="text" id="state">
                                    </div>
                                </div>
                            </div><!--/span-->
                        </div><!--/row-->
                        <div class="row-fluid">
                            <div class="span6 bgcolor">
                                <div class="control-group">
                                    <label class="control-label" for="postCode">CEP</label>
                                    <div class="controls">
                                        <input type="text" id="cep">
                                    </div>
                                </div>
                            </div><!--/span-->
                            <div class="span6 bgcolor">
                                <div class="control-group">
                                    <label class="control-label" for="country">País</label>
                                    <div class="controls">
                                        <select id="country">
                                            <option value="Brasil">Brasil</option>
                                        </select>
                                    </div>
                                </div>

                            </div><!--/span-->
                        </div><!--/row-->

                    </fieldset>
                <fieldset>
                    <legend><strong>USUÁRIO(OS)</strong></legend>
                    <div class="row-fluid">
                        <div class="span12 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="street">Endereço Completo</label>
                                <div class="controls">
                                    <input type="text" class="input-xxlarge" id="street">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="city">Cidade</label>
                                <div class="controls">
                                    <input type="text" id="city">
                                </div>
                            </div>
                        </div><!--/span-->
                        <div class="span6 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="state">UF</label>
                                <div class="controls">
                                    <input type="text" id="state">
                                </div>
                            </div>
                        </div><!--/span-->
                    </div><!--/row-->
                    <div class="row-fluid">
                        <div class="span6 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="postCode">CEP</label>
                                <div class="controls">
                                    <input type="text" id="cep">
                                </div>
                            </div>
                        </div><!--/span-->
                        <div class="span6 bgcolor">
                            <div class="control-group">
                                <label class="control-label" for="country">País</label>
                                <div class="controls">
                                    <select id="country">
                                        <option value="Brasil">Brasil</option>
                                    </select>
                                </div>
                            </div>

                        </div><!--/span-->
                    </div><!--/row-->
                </fieldset>
                </div><!--/span-->

                <div class="span4">
                </div><!--/span-->
            </div><!--/row-->
        </form>
        <hr>
        <footer>
            <p>&copy; Kickstart Web 2012</p>
        </footer>
    </div><!--/.fluid-container-->
    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/Scripts/jquery-1.8.0.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>

</body>
</html>
